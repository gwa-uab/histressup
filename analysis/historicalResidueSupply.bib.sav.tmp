@Article{Sharma2001,
  author   = {Sharma, Mahadev and Oderwald, Richard G},
  title    = {Dimensionally compatible volume and taper equations},
  year     = {2001},
  pages    = {8},
  abstract = {A dimensional analysis approach was applied to derive analytically consistent tree taper and volume equations. To achieve numerical consistency between the taper and volume equations, parameters of the taper and the volume equations were estimated simultaneously. Data from loblolly pine (Pinus taeda L,) trees grown in natural stands in the Coastal Plain of North Carolina and the Coastal Plain and Piedmont areas of Virginia were used to estimate the parameters. The diuiensionally compatible volume equation is shown to be a better equation for estimation of the volume of loblolly pine trees grown in these sites and can be applied for the estimation of total volume. The taper equation accurately predicts tree diameters from butt to the tree tip. It can be used to predict the diameter at any specified height and to predict height to any top diameter limit.},
