@Article{Papapostolou2011,
  author   = {Papapostolou, Christiana and Kondili, Emilia and Kaldellis, John K.},
  journal  = {Energy},
  title    = {Development and implementation of an optimisation model for biofuels supply chain},
  year     = {2011},
  issn     = {0360-5442},
  month    = oct,
  number   = {10},
  pages    = {6019--6026},
  volume   = {36},
  abstract = {Biofuels supply chain comprises a wide set of activities involving a rather complex set of parameters. Cultivation of the raw materials is closely related to the agricultural sector whereas the production of the final product presumes the operation of a conversion plant. The distribution network aims at delivering the final product close to the consumption. The extent of the involvement of each one of the previously mentioned sectors is the result of strategic and operational planning of the whole supply chain and, in the general case, determines the efficiency of the biofuels sector. Taking also into account the very rapidly changing opinions related to the environmental behaviour of the whole biofuels supply chain, it becomes very clear that the parameters in the sector are continuously changing. Therefore, the consideration of an integrated supply chain appropriately modelled is believed to be very critical and could result in the optimal solution per case, economically and/or environmentally speaking. In this paper the development of a mathematical model for the optimal design and operation of Biofuels Supply Chain is proposed as an integrated approach that can take into account both technical and economic parameters affecting the performance of the whole value chain. Model implementation would facilitate and support the decision taking in various planning and operational issues such as infrastructure investments, the quantities of raw materials to be cultivated, the quantities of biofuels to be produced in the domestic market or imported, identifying the best available solution for the optimal design and operation of the biofuels supply chain.},
  doi      = {10.1016/j.energy.2011.08.013},
  file     = {ScienceDirect Snapshot:C\:\\Users\\gwa_ualberta\\Zotero\\storage\\YFPEC9JS\\S0360544211005433.html:text/html;ScienceDirect Full Text PDF:C\:\\Users\\gwa_ualberta\\Zotero\\storage\\24YVFEQ7\\Papapostolou et al. - 2011 - Development and implementation of an optimisation .pdf:application/pdf},
  keywords = {Biodiesel conversion, Integrated biofuels planning, Transportation fuels},
  language = {en},
  url      = {https://www.sciencedirect.com/science/article/pii/S0360544211005433},
  urldate  = {2021-05-21},
}

@Article{Niquidet2014,
  author     = {Niquidet, Kurt and Friesen, Devon},
  journal    = {Canadian journal of forest research (Print)},
  title      = {Bioenergy potential from wood residuals in {Alberta}: a positive mathematical programming approach},
  year       = {2014},
  issn       = {0045-5067},
  month      = jan,
  note       = {Place: Ottawa, ON Publisher: National Research Council of Canada},
  number     = {12},
  pages      = {1586--1594},
  volume     = {44},
  abstract   = {Pour de nombreuses installations de bioénergie à base de bois, existantes et prévues, l'incertitude quant à l'approvisionnement futur en matière première constitue un risque majeur. Plusieurs projets de bioénergie utilisent des déchets générés par les secteurs primaires comme le bois de sciage et courent donc un risque inhérent aux fluctuations de l'approvisionnement liées à un changement dans ces industries. Pour évaluer la viabilité à long terme d'une installation de bioénergie à base de bois, il est nécessaire de comprendre comment fluctue la matière première de biomasse en fonction des autres secteurs et à quel coût l'offre devient accessible. Nous abordons ces questions en élaborant un modèle de programmation mathématique positive (PMP) pour le secteur forestier de l'Alberta en se concentrant sur l'optimisation des itinéraires de transfert de la fibre. À partir du modèle de PMP, nous obtenons une fonction de coût marginal pour la récolte et le transport de la fibre jusqu'à chaque usine de transformation. Les résultats indiquent que l'approvisionnement en résidus ligneux est très sensible aux conditions du marché dans le secteur primaire. Dans la majorité des cas, afin de soutenir l'expansion de la bioénergie, la matière première devra provenir de la forêt, puisque très peu de surplus de résidus de scieries sont disponibles même lorsque les prix du bois d'œuvre sont élevés. Cependant, nous estimons que le coût marginal pour fournir des résidus de récolte est important, de telle sorte qu'un soutien politique sera nécessaire pour le développement futur de la bioénergie.},
  file       = {EBSCO Full Text:C\:\\Users\\gwa_ualberta\\Zotero\\storage\\3IYWFHS9\\Niquidet and Friesen - 2014 - Bioenergy potential from wood residuals in Alberta.pdf:application/pdf},
  keywords   = {North America, Alberta, America, America del norte, Amérique, Amérique du Nord, Bioenergía, Bioénergie, Bioenergy, Biological and medical sciences, Bois, Canada, Canadá, Ciencias forestales, Ecologie, Ecology, Environment, Environnement, Foresterie, sylviculture, Forestry, silviculture, Fundamental and applied biological sciences. Psychology, Madera, Mathematical programming, Potencial, Potential, Potentiel, Programación matemática, Programmation mathématique, Sciences biologiques et medicales, Sciences biologiques fondamentales et appliquees. Psychologie, Sylviculture, Wood},
  shorttitle = {Bioenergy potential from wood residuals in {Alberta}},
  url        = {https://login.ezproxy.library.ualberta.ca/login?url=https://search.ebscohost.com/login.aspx?direct=true&db=edscal&AN=edscal.29130693&site=eds-live&scope=site},
  urldate    = {2021-05-21},
}

@Article{Niquidet2012,
  author     = {Niquidet, Kurt and Stennes, Brad and Kooten, G. C. van},
  journal    = {Canadian Journal of Agricultural Economics/Revue canadienne d'agroeconomie},
  title      = {Bioenergy from {Mountain} {Pine} {Beetle} {Timber} and {Forest} {Residuals}: {A} {Cost} {Analysis}},
  year       = {2012},
  issn       = {1744-7976},
  note       = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/j.1744-7976.2012.01246.x},
  number     = {2},
  pages      = {195--210},
  volume     = {60},
  abstract   = {In light of the large volumes of pine killed in the interior forests of British Columbia (BC) by the mountain pine beetle, many forest sector participants are keen to employ forest biomass as an energy source. To assess the feasibility of a wood biomass-fired power plant in the BC interior, it is necessary to know both how much physical biomass might be available over the life of a plant and its location as transportation cost is likely to be a major operating cost for any facility. To address these issues, we construct a mathematical programming model of fiber flows in the Quesnel Timber Supply Area of BC over a 25-year time horizon. The focus of the model is on minimizing the cost of supplying feedstock through space and time. Results indicate that over the life of the project, feedstock costs will more than double, increasing from \$54.60/bone-dry tonnes (BDt) (\$0.039/kWh) to \$116.14/BDt (\$0.083/kWh). En raison de l’important volume de pins dévastés par le dendroctone du pin ponderosa dans les forêts de l’intérieur de la Colombie-Britannique (BC), de nombreux participants du secteur forestier sont désireux d’utiliser la biomasse forestière comme source d’énergie. Pour évaluer la faisabilité d’une centrale alimentée à la biomasse ligneuse à l’intérieur de la Colombie-Britannique, il faut connaître la quantité de biomasse disponible pendant la durée de vie de la centrale ainsi que l’emplacement de cette biomasse étant donné que les coûts de transport risquent de représenter des coûts d’exploitation importants pour n’importe quelle installation. Afin d’examiner ces aspects, nous avons élaboré un modèle de programmation mathématique de l’approvisionnement en matière ligneuse dans la zone d’approvisionnement forestier de Quesnel en Colombie-Britannique, sur un horizon de 25 ans. Le modèle visait principalement à réduire le coût liéà l’approvisionnement en matière première à travers l’espace et le temps. D’après nos résultats, les coûts de la matière première feront plus que doubler au cours de la durée du projet, passant de 54,60 \$/tonne anhydre (t.a.) (0,039 \$/kWh) à 116,14 \$/t.a. (0,083 \$/kWh).},
  copyright  = {© 2012 Canadian Agricultural Economics Society},
  doi        = {https://doi.org/10.1111/j.1744-7976.2012.01246.x},
  file       = {Snapshot:C\:\\Users\\gwa_ualberta\\Zotero\\storage\\FJV8HVQS\\j.1744-7976.2012.01246.html:text/html},
  language   = {en},
  shorttitle = {Bioenergy from {Mountain} {Pine} {Beetle} {Timber} and {Forest} {Residuals}},
  url        = {https://onlinelibrary.wiley.com/doi/abs/10.1111/j.1744-7976.2012.01246.x},
  urldate    = {2021-05-21},
}

