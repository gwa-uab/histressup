@Article{Gronowska2009,
  author   = {{Magdalena Gronowska} and {Satish Joshi} and {Heather L. MacLean}},
  journal  = {BioResources},
  title    = {A {Review} of {U}.s. and {Canadian} {Biomass} {Supply} {Studies}},
  year     = {2009},
  issn     = {1930-2126},
  month    = feb,
  note     = {Publisher: North Carolina State University},
  number   = {1},
  pages    = {341--369},
  volume   = {4},
  abstract = {An improved understanding of lignocellulosic biomass availability is needed to support proposed expansion in biofuel production. Fifteen studies that estimate availability of lignocellulosic biomass quantities in in the U.S. and/or Canada are reviewed. Sources of differences in study methods and assumptions and resulting biomass quantities are elucidated. We differentiate between inventory studies, in which quantities of biomass potentially available are estimated without rigorous consideration of the costs of supply, versus economic studies, which take into consideration various opportunity costs and competition. The U.S. economic studies, which included reasonably comprehensive sets of biomass categories, estimate annual biomass availability to range from 6 million to 577 million dry metric tonnes (dry t), depending on offered price, while estimates from inventory studies range from 190 million to 3849 million dry t. The Canadian inventory studies, which included reasonably comprehensive sets of biomass categories, estimate availability to range from 64 million green t to 561 million dry t. The largest biomass categories for the U.S. are energy crops and agricultural residues, while for Canada they are expected to be energy crops and logging residues. The significant differences in study estimates are due in large part to the number of biomass categories included, whether economic considerations are incorporated, assump-tions about energy crop yields and land areas, and level of optimism of assumptions of the study.},
  file     = {EBSCO Full Text:C\:\\Users\\gwa_ualberta\\Zotero\\storage\\8F382B6T\\Magdalena Gronowska et al. - 2009 - A Review of U.s. and Canadian Biomass Supply Studi.pdf:application/pdf},
  keywords = {Biomass supply, Biotechnology, Lignocellulosic biomass, Resource assessment, TP248.13-248.65},
  url      = {https://login.ezproxy.library.ualberta.ca/login?url=https://search.ebscohost.com/login.aspx?direct=true&db=edsdoj&AN=edsdoj.928a8255c6ed4e9d84169d908580d2bc&site=eds-live&scope=site},
  urldate  = {2021-05-21},
}

@Article{Galik2009,
  author   = {Galik, Christopher S. and Abt, Robert and Wu, Yun},
  journal  = {Journal of Forestry},
  title    = {Forest {Biomass} {Supply} in the {Southeastern} {United} {States}—{Implications} for {Industrial} {Roundwood} and {Bioenergy} {Production}},
  year     = {2009},
  issn     = {0022-1201},
  month    = mar,
  number   = {2},
  pages    = {69--77},
  volume   = {107},
  abstract = {This analysis explores regional aggregate bioenergy potential, the interaction of logging residues and roundwood supply, and the potential supply costs of woody biomass from three southern states. Significant amounts of forest residues are potentially available within the study area: approximately 2.8 million dry tons in North Carolina, 1.8 million dry tons in South Carolina, and 1.3 million dry tons in Virginia. These quantities are sufficient to satisfy regional biomass electricity production requirements only through 2012 under a hypothetical national Renewable Portfolio Standard and Renewable Fuels Standard, after which changes in the biomass resource stream will be necessary. Supply curves generated for multiple regions in each state indicate that forest residues vary regionally in both supply and price, and exceeding the supply of forest residues could be accompanied by a dramatic spike in resource pricing, with implications for timberland owners and users of the forest resource base.},
  doi      = {10.1093/jof/107.2.69},
  file     = {Snapshot:C\:\\Users\\gwa_ualberta\\Zotero\\storage\\HQ2IS3FV\\4598896.html:text/html;Full Text PDF:C\:\\Users\\gwa_ualberta\\Zotero\\storage\\4FHGTV82\\Galik et al. - 2009 - Forest Biomass Supply in the Southeastern United S.pdf:application/pdf;deAvillez.pdf:C\:\\Users\\gwa_ualberta\\Zotero\\storage\\CWFUJ4G7\\deAvillez.pdf:application/pdf},
  url      = {https://doi.org/10.1093/jof/107.2.69},
  urldate  = {2021-05-21},
}

@Article{Demirbas2009,
  author   = {Demirbas, M. Fatih and Balat, Mustafa and Balat, Havva},
  journal  = {Energy Conversion and Management},
  title    = {Potential contribution of biomass to the sustainable energy development},
  year     = {2009},
  issn     = {0196-8904},
  month    = jul,
  number   = {7},
  pages    = {1746--1760},
  volume   = {50},
  abstract = {Biomass is a renewable energy source and its importance will increase as national energy policy and strategy focuses more heavily on renewable sources and conservation. Biomass is considered the renewable energy source with the highest potential to contribute to the energy needs of modern society for both the industrialized and developing countries worldwide. The most important biomass energy sources are wood and wood wastes, agricultural crops and their waste byproducts, municipal solid waste, animal wastes, waste from food processing, and aquatic plants and algae. Biomass is one potential source of renewable energy and the conversion of plant material into a suitable form of energy, usually electricity or as a fuel for an internal combustion engine, can be achieved using a number of different routes, each with specific pros and cons. Currently, much research has been focused on sustainable and environmental friendly energy from biomass to replace conventional fossil fuels. The main objective of the present study is to investigate global potential and use of biomass energy and its contribution to the sustainable energy development by presenting its historical development.},
  doi      = {10.1016/j.enconman.2009.03.013},
  file     = {ScienceDirect Snapshot:C\:\\Users\\gwa_ualberta\\Zotero\\storage\\INXYJV8F\\S0196890409000946.html:text/html;ScienceDirect Full Text PDF:C\:\\Users\\gwa_ualberta\\Zotero\\storage\\JSDA7DWU\\Demirbas et al. - 2009 - Potential contribution of biomass to the sustainab.pdf:application/pdf},
  keywords = {Bio-electricity, Biofuel, Biofuel economy, Biomass resources, Environmental impact},
  language = {en},
  url      = {https://www.sciencedirect.com/science/article/pii/S0196890409000946},
  urldate  = {2021-05-21},
}

@Article{Johnson2012,
  author   = {Johnson, Dana M. and Jenkins, Timothy L. and Zhang, Fengli},
  journal  = {Biofuels},
  title    = {Methods for optimally locating a forest biomass-to-biofuel facility},
  year     = {2012},
  issn     = {1759-7269},
  month    = jul,
  note     = {Publisher: Taylor \& Francis \_eprint: https://doi.org/10.4155/bfs.12.34},
  number   = {4},
  pages    = {489--503},
  volume   = {3},
  abstract = {A critical factor affecting the profitability of producing biofuel is optimally locating a forest biomass-to-biofuel facility. Spatial dispersion of forest biomass results in significant transportation cost of bulky, forest biomass feedstock. Multiple studies document different approaches to address theissues associated with biofuel facility siting. Study objectives were to: examine current literature (work published since 2005) discussing methods and models to determine the optimal location or site for a forest biomass-to-biofuel facility; compare the methods to identify the preferred approach; and identify potential gaps for future research. Our evaluation summarizes major, recent studies that focused on mathematical models to optimally locate a forest biomass-to-biofuel facility. Through analysis of the various methods, including the advantages and disadvantages, this study uncovered preferred approaches for locating an optimal biofuel facility location. It was found that optimization using mixed integer programming appears to be the preferred method for optimally locating a biofuel site.},
  doi      = {10.4155/bfs.12.34},
  file     = {Snapshot:C\:\\Users\\gwa_ualberta\\Zotero\\storage\\VRFIV3KQ\\bfs.12.html:text/html;Full Text PDF:C\:\\Users\\gwa_ualberta\\Zotero\\storage\\DKYEP73X\\Johnson et al. - 2012 - Methods for optimally locating a forest biomass-to.pdf:application/pdf;graonowska2009.pdf:C\:\\Users\\gwa_ualberta\\Zotero\\storage\\99SDWJFE\\graonowska2009.pdf:application/pdf;huang1992.pdf:C\:\\Users\\gwa_ualberta\\Zotero\\storage\\7B6RR7I3\\huang1992.pdf:application/pdf;jones2013.pdf:C\:\\Users\\gwa_ualberta\\Zotero\\storage\\4EUFV4S3\\jones2013.pdf:application/pdf;kim2011.pdf:C\:\\Users\\gwa_ualberta\\Zotero\\storage\\LE2FYUYS\\kim2011.pdf:application/pdf;kumar2003.pdf:C\:\\Users\\gwa_ualberta\\Zotero\\storage\\Z4EUD36M\\kumar2003.pdf:application/pdf;Methods for optimally locating a forest biomass to biofuel facility.pdf:C\:\\Users\\gwa_ualberta\\Zotero\\storage\\64KSYU4G\\Methods for optimally locating a forest biomass to biofuel facility.pdf:application/pdf;niquided2014.pdf:C\:\\Users\\gwa_ualberta\\Zotero\\storage\\44NRY2DJ\\niquided2014.pdf:application/pdf;papapostolou2011.pdf:C\:\\Users\\gwa_ualberta\\Zotero\\storage\\7AZJWDJB\\papapostolou2011.pdf:application/pdf;shabani2013.pdf:C\:\\Users\\gwa_ualberta\\Zotero\\storage\\ZJIR3SPA\\shabani2013.pdf:application/pdf;spelter2009.pdf:C\:\\Users\\gwa_ualberta\\Zotero\\storage\\RWW3VI5A\\spelter2009.pdf:application/pdf;stephen2010.pdf:C\:\\Users\\gwa_ualberta\\Zotero\\storage\\Z2QERMC3\\stephen2010.pdf:application/pdf;bokesjo2006.pdf:C\:\\Users\\gwa_ualberta\\Zotero\\storage\\RDP22LUU\\bokesjo2006.pdf:application/pdf;chen2012.pdf:C\:\\Users\\gwa_ualberta\\Zotero\\storage\\P35X3KYA\\chen2012.pdf:application/pdf;demirbas2009.pdf:C\:\\Users\\gwa_ualberta\\Zotero\\storage\\FTMY3B55\\demirbas2009.pdf:application/pdf;dymond2010.pdf:C\:\\Users\\gwa_ualberta\\Zotero\\storage\\HPUATZQS\\dymond2010.pdf:application/pdf;galik2009.pdf:C\:\\Users\\gwa_ualberta\\Zotero\\storage\\5FQBRAXE\\galik2009.pdf:application/pdf;Glen paper intro - draft 2.docx:C\:\\Users\\gwa_ualberta\\Zotero\\storage\\C7IEKVFY\\Glen paper intro - draft 2.docx:application/vnd.openxmlformats-officedocument.wordprocessingml.document},
