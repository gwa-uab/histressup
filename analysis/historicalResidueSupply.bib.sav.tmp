@Article{Buongiorno2011,
  author     = {Buongiorno, Joseph and Raunikar, Ronald and Zhu, Shushuai},
  journal    = {Journal of Forest Economics},
  title      = {Consequences of increasing bioenergy demand on wood and forests: {An} application of the {Global} {Forest} {Products} {Model}},
  year       = {2011},
  issn       = {1104-6899},
  month      = apr,
  number     = {2},
  pages      = {214--229},
  volume     = {17},
  abstract   = {The Global Forest Products Model (GFPM) was applied to project the consequences for the global forest sector of doubling the rate of growth of bioenergy demand relative to a base scenario, other drivers being maintained constant. The results showed that this would lead to the convergence of the price of fuelwood and industrial roundwood, raising the price of industrial roundwood by nearly 30\% in 2030. The price of sawnwood and panels would be 15\% higher. The price of paper would be 3\% higher. Concurrently, the demand for all manufactured wood products would be lower in all countries, but the production would rise in countries with competitive advantage. The global value added in wood processing industries would be 1\% lower in 2030. The forest stock would be 2\% lower for the world and 4\% lower for Asia. These effects varied substantially by country.},
  doi        = {10.1016/j.jfe.2011.02.008},
  file       = {ScienceDirect Snapshot:C\:\\Users\\gwa_ualberta\\Zotero\\storage\\8Y7A43HT\\S1104689911000109.html:text/html},
  keywords   = {Bioenergy, Demand, Forest sector, Fuelwood, Modeling, Prices, Supply, Trade, Wood products},
  language   = {en},
  series     = {Fuelwood, timber and climate change: {Insights} from the forest sector modeling},
  shorttitle = {Consequences of increasing bioenergy demand on wood and forests},
  url        = {https://www.sciencedirect.com/science/article/pii/S1104689911000109},
  urldate    = {2021-05-21},
}

@Article{Bolkesjo2006,
  author     = {Folsland Bolkesjø, Torjus and Trømborg, Erik and Solberg, Birger},
  journal    = {Scandinavian Journal of Forest Research},
  title      = {Bioenergy from the forest sector: {Economic} potential and interactions with timber and forest products markets in {Norway}},
  year       = {2006},
  issn       = {02827581},
  month      = apr,
  note       = {Publisher: Taylor \& Francis Ltd},
  number     = {2},
  pages      = {175--185},
  volume     = {21},
  abstract   = {A partial equilibrium forest sector model which is augmented to include bioenergy was applied to project the use of bioenergy based on forest fuels and forest industry by-products in Norway for three different scenarios of the future prices of electricity and oil. The impacts on forestry and forest industries of the different energy price scenarios were also studied. The advantage of the suggested methodology is that it allows for assessments of the economic potential of bioenergy, taking into account the competition for raw materials, the specific heat demand of various regions, and interregional and international trade. Bioenergy will, according to this study, be fairly competitive in some market segments with the current price levels of electricity and oil, and only a minor increase (decrease) in energy (roundwood) prices would release substantially increased bioenergy production levels. Pulpwood prices of pine and non-coniferous species are projected to increase substantially when assuming increasing energy prices. Except for particleboard mills, production levels of forest industries appeared relatively insensitive to the energy price changes.},
  doi        = {10.1080/02827580600591216},
  file       = {EBSCO Full Text:C\:\\Users\\gwa_ualberta\\Zotero\\storage\\3KJRQ7YJ\\Folsland Bolkesjø et al. - 2006 - Bioenergy from the forest sector Economic potenti.pdf:application/pdf},
  keywords   = {Bioenergy, BIOMASS energy, economic potential, ECONOMIC structure, ENERGY industries, forest industry, FOREST products industry, FORESTS \& forestry, METHODOLOGY, NORWAY, partial equilibrium model, PRICING, RAW materials, timber prices},
  shorttitle = {Bioenergy from the forest sector},
  url        = {https://login.ezproxy.library.ualberta.ca/login?url=https://search.ebscohost.com/login.aspx?direct=true&db=a9h&AN=20917529&site=eds-live&scope=site},
  urldate    = {2021-05-21},
}

